{
  "expo": {
    "name": "AMSVault",
    "slug": "amsvault-app",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "amsvault",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2196F3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon.png",
      "bundleIdentifier": "com.amsvault.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#2196F3"
      },
      "package": "com.amsvault.app",
      "icon": "./assets/icon.png"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a59ad43e-1ec5-45d0-862a-00f2befe87f6"
      }
    }
  }
}
