{
  "expo": {
    "name": "AMSVault",
    "slug": "amsvault-app",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "amsvault",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2563eb"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2563eb"
      },
      "package": "com.example.amsvault"
    },
    "web": {
      "bundler": "metro"
    }
  }
}
